<script type="text/javascript">


// define options (if needed)
// Initializes and opens PhotoSwipe
$(function() {
  var gallery = null;
  var gallery_options = {};

  $('.item-slide').on("click",function(evt){
    var index = $(this).data("index");
    var origTarget = evt.target;

    if (origTarget && origTarget.href) {
      return;
    }

    gallery_options = {galleryPIDs:true, history:true,index:index};
    gallery = new PhotoSwipe( pswpElement, PhotoSwipeUI_Default, galleryItems, gallery_options);
    gallery.init();

  });

  var hashData = getHashData();
  if(hashData != null) {


    var index = 0;
    for (index = 0; index < galleryItems.length; ++index) {
      var galleryItem = galleryItems[index];

      if(galleryItem.pid == hashData){
        break;
      }
    }

        gallery_options = {galleryPIDs:true, history:true,index:index};
        gallery = new PhotoSwipe( pswpElement, PhotoSwipeUI_Default, galleryItems, gallery_options);
        gallery.init();
  }

  $('.items_per_page').on('change', function() {
    $(this).parents('form').submit()
  });

});
</script>




