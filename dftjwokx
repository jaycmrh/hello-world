<script>
  var sendMfcMailModal = function(to,object_type,object_slug){
    $('#mfc-mail-modal .mfc-mail-to').text(to);
    $('#mfc-mail-modal #mfc_mail_to').val(to);
    $('#mfc-mail-modal #mfc_mail_message').val(null);
    $('#mfc-mail-modal #mfc_mail_object_type').val(object_type);
    $('#mfc-mail-modal #mfc_mail_object_slug').val(object_slug);
    $('#mfc-mail-modal').modal("show");
  }


  $('#mfc_mail_form').bind("ajax:success", function(xhr, data, status) {
    $('#mfc-mail-modal').modal("hide");
  });
</script>

