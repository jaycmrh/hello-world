<script type="text/javascript">
  $('#share-preview-tweet').on('input', function(e) {
      var tw = twttr.txt.parseTweet( $('#share-preview-tweet').val() );
      if ( !tw.valid ) {
          $('#share-tweet-errors').html("Invalid Tweet length");
          $('#share-tweet-errors').removeClass("hide");
        } else {
          $('#share-tweet-errors').addClass("hide");
        }

        $('#share-tweet-length').html(tw.weightedLength);  
  });
 
</script>


